  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch main.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\cli\\main.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch main.js MOCK",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\cli\\main.js",
      "args": ["MOCK"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch display-results.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\cli\\display-results.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch interface.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\cli\\interface.js"
    }
  ]
}